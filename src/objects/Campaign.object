{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/apex.js&quot;)} 

var fnSuccess = function() { 
    location.href = &quot;701/o&quot;; 
}; 

console.log(&quot;{!Campaign.RecordType}&quot;);
console.log(&quot;{!Campaign.IsActive}&quot;);
if(&quot;{!Campaign.RecordType}&quot; == &quot;Central Marketing Campaign&quot; &amp;&amp; &quot;{!Campaign.IsActive}&quot; == 1){
   console.log(3);
   alert(&quot;{!$Label.Cannot_Delete_Central_Campaign}&quot;);
} else {   
   var result = sforce.connection.deleteIds([&quot;{!Campaign.Id}&quot;]);
 if (result [0].getBoolean(&quot;success&quot;)) {
    alert(&quot;{!$Label.Delete_Campaign_Success}&quot;);
    fnSuccess();
  } else {
    alert(&quot;{!$Label.Delete_Campaign_Failed}&quot; + &quot;\n&quot; + result[0].errors.message);
  }
}</url>
