    <validationRules>
        <fullName>Only_Started_Campaign_can_be_activated</fullName>
        <active>true</active>
        <description>When Status = Planned, Confirmed, Approved, Closed, Cancelled, Active can not be true.
Record Type = CAC CRM, Central Campaign, CAS Campaign</description>
        <errorConditionFormula>NOT(ISPICKVAL( Status , &apos;Started&apos;)) 
&amp;&amp; IsActive
&amp;&amp; ( RecordTypeId = &quot;012N0000000ClJT&quot; || RecordTypeId = &quot;012N0000000Cl19&quot; || RecordTypeId = &quot;012N0000000ClJO&quot; )</errorConditionFormula>
        <errorDisplayField>IsActive</errorDisplayField>
        <errorMessage>Only Campaign with &quot;Started&quot; status can be activated.</errorMessage>
    </validationRules>
