IF(ISPICKVAL(Standard_Opening__c, &apos;高&apos;), 1, 0) + 
IF(ISPICKVAL(Proper_Pronunciation_Intonation_Speed__c, &apos;高&apos;), 1, 0) + 
IF(ISPICKVAL(Specification_Polite_Language__c, &apos;高&apos;), 1, 0) + 
IF(ISPICKVAL(Specification_Spoken_Language__c, &apos;高&apos;), 1, 0) + 
IF(ISPICKVAL(Standard_Conclusion__c, &apos;高&apos;), 1, 0) + 
IF(ISPICKVAL(Communicate_Ability_Listening_Ability__c, &apos;高&apos;), 1, 0) + 
IF(ISPICKVAL(Communicate_Ability_Initiative__c, &apos;高&apos;), 1, 0) + 
IF(ISPICKVAL(Communicate_Ability_Rapid_Response__c, &apos;高&apos;), 1, 0) + 
IF(ISPICKVAL(Perception_Focus_On_Customer_Feel__c, &apos;高&apos;), 1, 0) + 
IF(ISPICKVAL(Perception_Patience_Degree__c, &apos;高&apos;), 1, 0) + 
IF(ISPICKVAL(Perception_JinYongYu__c, &apos;高&apos;), 1, 0) + 
IF(ISPICKVAL(Perception_Hang_Up__c, &apos;高&apos;), 1, 0) 
